
EJ 4
SELECT BARES.NOMBRE,BARES.PRESUPUESTO
FROM PARRANDEROS.BARES 
WHERE BARES.CIUDAD= 'Bogotá'

EJ 5
SELECT BEBEDORES.NOMBRE
FROM PARRANDEROS.BEBEDORES
WHERE BEBEDORES.PRESUPUESTO IS NULL

EJ 6 
SELECT BARES.NOMBRE
FROM PARRANDEROS.BARES
WHERE BARES.CIUDAD!= 'Bogotá' AND BARES.CIUDAD!= 'Medellín"

EJ 7 
SELECT FRECUENTAN.*
FROM PARRANDEROS.FRECUENTAN
ORDER BY FRECUENTAN.ID_BAR ASC ,FRECUENTAN.FECHA_ULTIMA_VISITA DESC

EJ 8 
SELECT BEBEDORES.*
FROM PARRANDEROS BEBEDORES 
ORDER BY BEBEDORES.NOMBRE ASC, BEBEDORES.PRESUPUESTO DESC

EJ 9 
SELECT BEBEDORES .*
FROM BEBEDORES 
WHERE SUBSTR (BEBEDORES.NOMBRE ,1,1) = 'A'

EJ 10 
SELECT FRECUENTAN.* 
FROM PARRANDEROS.FRECUENTAN
WHERE FRECUENTAN.FECHA_ULTIMA_VISITA>'13-12-2011' or FRECUENTAN.HORARIO = 'diurno'


EJ 11
SELECT SUM (BARES.cant:sedes)
FROM PARRANDEROS.BARES
